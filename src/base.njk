<!DOCTYPE html>
<html dir="ltr" lang="es-ES">
<head>

	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta name="author" content="Red Ciudadana" />
  <meta content="width=device-width, initial-scale=1" name="viewport" />

	<link rel="icon" href="/elementos/redciudadana.png" type="image/png">

	<!-- Stylesheets
	============================================= -->

	<!-- / -->
	<link rel="stylesheet" href="/src/css/style.css">
	<link rel="stylesheet" href="/src/css/main.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    {% block head %}
    {% endblock %}

</head>

<body class="stretched search-overlay">
	
	<!-- Preloader Start -->
    <div id="preloader-active">
      <div class="preloader d-flex align-items-center justify-content-center">
        <div class="preloader-inner position-relative">
          <div class="text-center">
            <img src="/src/assets/loader.gif" style="width:50%;height:auto;margin:auto" alt="" />
          </div>
        </div>
      </div>
    </div>
    <header id="header" class="border-0" data-mobile-sticky="true">

        <nav class="navbar sticky navbar-expand-lg navbar-light bg-white">
            
            <!--LOGO-->
            <div class="logo" style="align-items:center">
              <a class="navbar-brand" style="margin-left:5%" href="/index.html"><img style="max-width:200px!important" src="/src/assets/logos/pp.png"></a>
              <a class="navbar-brand" style="margin-left:5%" href="https://redciudadana.org/"><img style="max-width:200px!important" src="/src/assets/logos/red_negro.png"></a>
            </div>
            <!--RESPONSIVE-->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>

            <!--MENU-->
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav ml-auto" style="margin-right: 10%;">
                    <a class="nav-item nav-link " href="/index.html">Inicio</a>
                    <a class="nav-item nav-link" href="/partidos/index.html">Partidos Políticos</a>
                    <a class="nav-item nav-link" href="/comites/index.html">Comites Cívicos</a>
                    <a class="nav-item nav-link" href="/once/index.html">11 reglas</a>
                    <a class="nav-item nav-link" href="/contacto/index.html">Contacto</a>


                    <!--Redes-->
                    <div class="redes_header">
                      <a class="nav-item nav-link" target="_blank" href="https://www.facebook.com/Redciudadanagt/"><i class="fab fa-facebook-f" style="font-size: 20px;"></i> </a>
                      <a class="nav-item nav-link" target="_blank" href="https://twitter.com/RedxGuate"><i class="fab fa-twitter" style="font-size: 20px;"></i></a>
                      <a class="nav-item nav-link" target="_blank" href="https://www.instagram.com/redxguate/?hl=es"><i class="fab fa-instagram" style="font-size: 20px;"></i></a>
                    </div>

                </div>
            </div>
        </nav>


    </header><!-- #header end -->

    <main>
		
		{% block content %}

        {% endblock %} 

	</main>

	<!-- footer-area-start -->
    <footer>
		<div class="footer-middle-area pt-40 pb-5" style="background-image:url(/src/assets/SLIDER-08.png); background-color:#202d3e  ">
			<div class="container">
				<div class="row" style="justify-content:space-between">
					<div class="col-xl-4 col-lg-4 col-md-4 col-sm-12">
						<div class="footer-wrapper mb-30">
							<div class="footer-logo">
							<a href="https://redciudadana.org/"><img src="/src/assets/LOGO-RED_BLANCO.png" alt=""></a>
							</div>
						</div>
					</div>
					<div class="col-xl-3 col-lg-3 col-md-3 col-sm-12">
						<div class="d-flex align-items-center justify-content-end">
							<a style="margin:0 3px" target="_blank" href="https://www.facebook.com/Redciudadanagt"><img src="/src/assets/REDES-27.png"></a>
							<a style="margin:0 3px" target="_blank" href="https://twitter.com/redxguate"><img src="/src/assets/REDES-28.png"></a>
							<a style="margin:0 3px" target="_blank" href="https://www.instagram.com/redxguate/"><img src="/src/assets/REDES-29.png"></a>
							<a style="margin:0 3px" target="_blank" href="https://www.youtube.com/channel/UCQwc62j7beStZYFzwPxBEQg"><img src="/src/assets/REDES-30.png"></a>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-xl-12 col-lg-12 col-md-12">
						<div class="linea-footer"></div>
					</div>
				</div>
				<div class="footer-bottom-area pt-25 mt-20" style="border-top:none">
					<div class="row">
						<div class="col-xl-12">
							<div class="copyright text-center">
								<a href="https://redciudadana.org/"><p style="color:#fff!important">Asociación Civil Red Ciudadana</p></a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
    </footer>
    <!-- footer-area-end -->

	<button onclick="topFunction()" id="goToTop" title="Go to top"><i class="fa-solid fa-arrow-turn-up"></i></button>


    <!--js-->   
    <script src="/src/js/goToTop.js" type="text/javascript"></script> 

    <script src="/src/js/jquery.js"></script>
   

	<script>
      (function ($) {
        ("use strict");
        // Page loading
        $(window).on("load", function () {
          $("#preloader-active").delay(450).fadeOut("slow");
          $("body").delay(450).css({
            overflow: "visible"
          });
        });
      
      })(jQuery);
    </script>


    <script>
      if (window.netlifyIdentity) {
          window.netlifyIdentity.on('init', (user) => {
          if (!user) {
              window.netlifyIdentity.on('login', () => {
              document.location.href = '/admin/';
              });
          }
          });
      }
    </script>
    
    <script src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
	
	
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    {% block body_javascripts %}

    {% endblock %}

	</body>
</html>
